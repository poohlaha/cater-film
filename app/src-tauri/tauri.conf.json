{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:9999",
    "distDir": "../../web/dist"
  },
  "package": {
    "productName": "cater-film",
    "version": "1.0.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.test.app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "iOS": {
        "developmentTeam": "123123123"
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "app",
        "width": 800,
        "height": 600,
        "titleBarStyle": "Transparent"
      }
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
